<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.wp.dao.EmailDao">

    <insert id="insertCodeMail" parameterType="cn.wp.model.Email">
        insert into email (mail_address,code,create_at) VALUES (#{mailAddress},#{code},#{createAt})
    </insert>
</mapper>