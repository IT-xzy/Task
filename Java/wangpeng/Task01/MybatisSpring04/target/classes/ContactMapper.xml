<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.wp.dao.ContactDao">
    <insert id="save" parameterType="cn.wp.entity.Contact">
        insert into Contact (ID,Name,Email,QQ,Gender) value (#{ID},#{Name},#{Email},#{QQ},#{Gender})
    </insert>
    <select id="getContact" parameterType="int" resultType="cn.wp.entity.Contact">
        select * from Contact where id =#{ID}
    </select>

 </mapper>