<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.UserMapper">
    <select id="selectName" resultType="User" parameterType="String">
        SELECT * from customer WHERE name = #{name}
    </select>

     <select id="insertUser" parameterType="User">
         INSERT INTO customer(name,password,salt,create_time,update_time)
           VALUES(#{name},#{password},#{salt},#{create_time},#{update_time})
     </select>


</mapper>