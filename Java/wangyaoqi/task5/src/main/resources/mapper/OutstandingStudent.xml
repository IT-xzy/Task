<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wyq.webapp.mapper.OutstandingStudentMapper">
    <select id="get"  resultType="OutstandingStudent">
        select * from outstanding_student
          <if test="start!=null and count!=null">
              limit #{start},#{count}
          </if>
    </select>
    <select id="total" resultType="int">
        select count(0) from outstanding_student
    </select>
</mapper>