*{
  box-sizing: border-box;
}
body,
p,
h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
  padding: 0;
  font-weight: normal;
}
ul,
ol,
li {
  margin: 0;
  padding: 0;
  list-style-type: none;
}
a,
a:hover {
  display: inline-block;
  cursor: pointer;
  text-decoration: none;
  color: #fff;
}
span{
  display: inline-block;
}
img{
  max-width: 100%;
}